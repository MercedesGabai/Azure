				"typeProperties": {
					"location": {
						"type": "FileServerLocation",
						"fileName": "time_series_covid19_recovered_global.csv",
						"folderPath": "Johnhopkins"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": []
		}
	]
}